             mc:Ignorable="d" 
             d:DesignHeight="201" d:DesignWidth="140">
    <Grid>
        
        <Button Content="#" Height="54"
                HorizontalAlignment="Left" Margin="22,12,0,0" Name="sensorCodeBT" VerticalAlignment="Top" Width="97" />
        <CheckBox Content="CycleTime" Height="16" HorizontalAlignment="Left" Margin="22,111,0,0" Name="cycleTimeCB" VerticalAlignment="Top" />
        <CheckBox Content="MovingTime" Height="16" HorizontalAlignment="Left" Margin="22,133,0,0" Name="movingTimeCB" VerticalAlignment="Top" />
        <CheckBox Content="ScramTime" Height="16" HorizontalAlignment="Left" Margin="22,155,0,0" Name="scramTimeCB" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="22,80,0,0" Name="codeTB" VerticalAlignment="Top" Width="106" />
         
    </Grid>
